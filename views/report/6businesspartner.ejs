<html>

<head>
    <title>Data Business Partner</title>

    <link href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/css/bootstrap.min.css" rel="stylesheet">

</head>

<body>
    <h1>Data Business Partner</h1>
    <br>
    <table class="table table-bordered">
        <thead>
            <tr>
                <th scope="col">No</th>
                <th scope="col">Label</th>
                <th scope="col">Value</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>1</td>
                <td>Code</td>
                <td><%= posts.businesspartner.businesspartner_code %></td>
            </tr>
            <tr>
                <td>2</td>
                <td>Name</td>
                <td><%= posts.businesspartner.name %></td>
            </tr>
            <tr>
                <td>3</td>
                <td>Category</td>
                <td><%= posts.businesspartner.businesspartner_category %></td>
            </tr>
            <tr>
                <td>4</td>
                <td>Salesman</td>
                <td><%= posts.businesspartner.salesman_employee %></td>
            </tr>
            <tr>
                <td>5</td>
                <td>Dept Collector</td>
                <td><%= posts.businesspartner.collector_employee %></td>
            </tr>
            <tr>
                <td>6</td>
                <td>Group</td>
                <td><%= posts.businesspartner.businesspartner_group %></td>
            </tr>
            <tr>
                <td>6</td>
                <td>Address</td>
                <td>
                    <ul>
                        <% for(let i = 0; i < posts.businesspartner_address.length; i++) { %>
                            <%= posts.businesspartner_address[i].name %> -
                            <%= posts.businesspartner_address[i].address %>
                        <% } %>
                    </ul>
                </td>
            </tr>
            <tr>
                <td>6</td>
                <td>Contact</td>
                <td>

                    <ul>
                        <% for(let i = 0; i < posts.businesspartner_contact.length; i++) { %>
                            <%= posts.businesspartner_contact[i].name %> -
                            <%= posts.businesspartner_contact[i].telp1 %>
                        <% } %>
                    </ul>

                </td>
            </tr>
            <tr>
                <td>7</td>
                <td>Description</td>
                <td><%= posts.businesspartner.description %></td>
            </tr>
        </tbody>
    </table>
    <br>
    <div class="row">
        <div class="col-sm">
            Print On <%= posts.currentDate %>
        </div>
    </div>
</body>

</html>